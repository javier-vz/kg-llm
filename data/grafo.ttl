  rdfs:label "incluye distrito"@es .

:incluyeComunidad a owl:ObjectProperty ;
  rdfs:label "incluye comunidad"@es .

:incluyeParada a owl:ObjectProperty ;
  rdfs:label "incluye parada"@es .

:incluyeObjetoRitual a owl:ObjectProperty ;
  rdfs:label "incluye objeto ritual"@es .

:seDejaImagenEn a owl:ObjectProperty ;
  rdfs:label "se deja imagen en"@es .

:seCustodiaEn a owl:ObjectProperty ;
  rdfs:label "se custodia en"@es .

:seCantaEn a owl:ObjectProperty ;
  rdfs:label "se canta en"@es .

:usaNombreLocal a owl:ObjectProperty ;
  rdfs:label "usa nombre local"@es .

:esVeneradaEn a owl:ObjectProperty ;
