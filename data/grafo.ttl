:documentaA a owl:ObjectProperty ;
  rdfs:domain :RecursoMedial ;
  rdfs:range owl:Thing ;
  rdfs:label "documenta a"@es .

:participaEnFestividad a owl:ObjectProperty ;
  rdfs:label "participa en festividad"@es ;
