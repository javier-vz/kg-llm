@prefix : <http://example.org/festividades#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

#################################################################
### 1. CLASES (ONTOLOGÍA)
#################################################################

:Festividad a owl:Class ;
    rdfs:label "Festividad"@es .

:PersonajeRitual a owl:Class ;
    rdfs:label "Personaje ritual"@es .

:Danza a owl:Class ;
    rdfs:label "Danza"@es .

:Comparsa a owl:Class ;
    rdfs:label "Comparsa"@es .

:EntidadVenerada a owl:Class ;
    rdfs:label "Entidad venerada"@es .

:TiempoRitual a owl:Class ;
    rdfs:label "Tiempo ritual"@es .

:Procesion a owl:Class ;
    rdfs:label "Procesión"@es .

:Santuario a owl:Class ;
    rdfs:label "Santuario"@es .

:ObjetoRitual a owl:Class ;
    rdfs:label "Objeto ritual"@es .

:VestimentaRitual a owl:Class ;
    rdfs:label "Vestimenta ritual"@es .

:RolRitual a owl:Class ;
    rdfs:label "Rol ritual"@es .

:RutaPeregrinacion a owl:Class ;
    rdfs:label "Ruta de peregrinación"@es .

:LugarDePaso a owl:Class ;
    rdfs:label "Lugar de paso"@es .

:Iglesia a owl:Class ;
    rdfs:label "Iglesia"@es .

:Comunidad a owl:Class ;
    rdfs:label "Comunidad"@es .

:PersonaParticipante a owl:Class ;
    rdfs:label "Persona participante"@es .

:RecursoMedial a owl:Class ;
    rdfs:label "Recurso medial"@es .

#################################################################
### 2. SUBCLASES
#################################################################

:DanzaQhapaqQolla a owl:Class ;
    rdfs:subClassOf :Danza ;
    rdfs:label "Danza Qhapaq Qolla"@es .

:DanzaUkuku a owl:Class ;
    rdfs:subClassOf :Danza ;
    rdfs:label "Danza Ukuku"@es .

:PersonajeUkuku a owl:Class ;
    rdfs:subClassOf :PersonajeRitual ;
    rdfs:label "Personaje Ukuku"@es .

:PersonajeQolla a owl:Class ;
    rdfs:subClassOf :PersonajeRitual ;
    rdfs:label "Personaje Qolla"@es .

:MascararRitual a owl:Class ;
    rdfs:subClassOf :ObjetoRitual ;
    rdfs:label "Máscara ritual"@es .

:RolGuardian a owl:Class ;
    rdfs:subClassOf :RolRitual ;
    rdfs:label "Rol de guardián"@es .

:SegmentoRitual a owl:Class ;
    rdfs:subClassOf :TiempoRitual ;
    rdfs:label "Segmento ritual"@es .

:Foto a owl:Class ;
    rdfs:subClassOf :RecursoMedial ;
    rdfs:label "Fotografía"@es .

:Video a owl:Class ;
    rdfs:subClassOf :RecursoMedial ;
    rdfs:label "Video"@es .

:Audio a owl:Class ;
    rdfs:subClassOf :RecursoMedial ;
    rdfs:label "Audio"@es .

:Documento a owl:Class ;
    rdfs:subClassOf :RecursoMedial ;
    rdfs:label "Documento"@es .

#################################################################
### 3. PROPIEDADES DE OBJETO
#################################################################

:realizaDanza a owl:ObjectProperty ;
    rdfs:label "realiza danza"@es .

:perteneceAComparsa a owl:ObjectProperty ;
    rdfs:label "pertenece a comparsa"@es .

:SeCelebraEn a owl:ObjectProperty ;
    rdfs:label "se celebra en"@es .

:ocurreDurante a owl:ObjectProperty ;
    rdfs:label "ocurre durante"@es .

:representaEntidad a owl:ObjectProperty ;
    rdfs:label "representa entidad"@es .

:usaObjetoRitual a owl:ObjectProperty ;
    rdfs:label "usa objeto ritual"@es .

:UbicadoEn a owl:ObjectProperty ;
    rdfs:label "ubicado en"@es .

:estaEnLugar a owl:ObjectProperty ;
    rdfs:label "está en lugar"@es .

:relacionadoCon a owl:ObjectProperty ;
    rdfs:label "relacionado con"@es .

:narradoPor a owl:ObjectProperty ;
    rdfs:label "narrado por"@es .

:documentaA a owl:ObjectProperty ;
    rdfs:label "documenta a"@es .

:capturadoEnSegmento a owl:ObjectProperty ;
    rdfs:label "capturado en segmento ritual"@es .

:tieneRecursoMedial a owl:ObjectProperty ;
    rdfs:label "tiene recurso medial"@es .

#################################################################
### 3bis. PROPIEDADES DE ANOTACIÓN (PARA TEXTO / EMBEDDINGS)
#################################################################

:descripcionBreve a owl:AnnotationProperty ;
    rdfs:label "Descripción breve"@es .

:descripcionEtnografica a owl:AnnotationProperty ;
    rdfs:label "Descripción etnográfica"@es .

:fuenteTexto a owl:AnnotationProperty ;
    rdfs:label "Fuente del texto"@es .

#################################################################
### 4. INDIVIDUOS (INSTANCIAS)
#################################################################

### Festividades
:FestividadQoylluriti a :Festividad ;
    rdfs:label "Festividad del Señor de Qoyllur Rit'i"@es .

:FestividadVirgenCarmenPaucartambo a :Festividad ;
    rdfs:label "Festividad de la Virgen del Carmen de Paucartambo"@es .

### Lugares
:SantuarioQoylluriti a :Santuario ;
    rdfs:label "Santuario del Señor de Qoyllur Rit'i"@es .

:IglesiaPaucartambo a :Iglesia ;
    rdfs:label "Iglesia de Paucartambo"@es .

:EntradaPrincipalIglesia a :LugarDePaso ;
    rdfs:label "Entrada principal de la iglesia"@es .

### Tiempos / segmentos rituales
:MomentoAlba a :SegmentoRitual ;
    rdfs:label "Momento del alba"@es .

### Comparsas
:ComparsaUkukus2024 a :Comparsa ;
    rdfs:label "Comparsa Ukukus 2024"@es .

### Personas y personajes
:PersonajeUkuku1 a :PersonajeUkuku ;
    rdfs:label "Ukuku (individuo 1)"@es .

:PersonajeQolla1 a :PersonajeQolla ;
    rdfs:label "Qolla (individuo 1)"@es .

:UkukuTestimonio1 a :PersonaParticipante ;
    rdfs:label "Persona que relata como Ukuku"@es .

### Danzas específicas
:DanzaUkukuInstancia a :DanzaUkuku ;
    rdfs:label "Danza Ukuku (instancia)"@es .

:DanzaQollaInstancia a :DanzaQhapaqQolla ;
    rdfs:label "Danza Qhapaq Qolla (instancia)"@es .

### Objetos y vestimenta
:MascararUkuku1 a :MascararRitual ;
    rdfs:label "Máscara de Ukuku"@es .

:VestimentaUkuku1 a :VestimentaRitual ;
    rdfs:label "Vestimenta ritual de Ukuku"@es .

### Comunidad
:ComunidadPaucartambo a :Comunidad ;
    rdfs:label "Comunidad de Paucartambo"@es .

### Recursos mediales de ejemplo
:FotoEntradaNaciones_001 a :Foto ;
    rdfs:label "Foto de la Entrada de Naciones al alba"@es .

:AudioTestimonioUkuku_001 a :Audio ;
    rdfs:label "Audio de testimonio de Ukuku"@es .

:DocumentoProgramaFiesta_2024 a :Documento ;
    rdfs:label "Programa oficial de la fiesta 2024"@es .

#################################################################
### 5. RELACIONES (AXIOMAS)
#################################################################

### Personajes → danzas, comparsas, objetos, entidades
:PersonajeUkuku1
    :realizaDanza :DanzaUkukuInstancia ;
    :perteneceAComparsa :ComparsaUkukus2024 ;
    :usaObjetoRitual :MascararUkuku1 ;
    :representaEntidad :EntidadVenerada .

:PersonajeQolla1
    :realizaDanza :DanzaQollaInstancia ;
    :perteneceAComparsa :ComparsaUkukus2024 .

### Festividades → lugares y tiempos
:FestividadQoylluriti
    :SeCelebraEn :SantuarioQoylluriti ;
    :ocurreDurante :MomentoAlba ;
    :tieneRecursoMedial :FotoEntradaNaciones_001 .

:FestividadVirgenCarmenPaucartambo
    :SeCelebraEn :IglesiaPaucartambo ;
    :ocurreDurante :MomentoAlba ;
    :tieneRecursoMedial :DocumentoProgramaFiesta_2024 .

### Procesiones, rutas, lugares
:EntradaDeNaciones a :Procesion ;
    rdfs:label "Entrada de Naciones"@es ;
    :ocurreDurante :MomentoAlba ;
    :estaEnLugar :EntradaPrincipalIglesia .

### Comunidad → festividad
:ComunidadPaucartambo :relacionadoCon :FestividadQoylluriti .

### Testimonios y recursos mediales
:FestividadQoylluriti :narradoPor :UkukuTestimonio1 .

:FotoEntradaNaciones_001
    :documentaA :EntradaDeNaciones ;
    :capturadoEnSegmento :MomentoAlba .

:AudioTestimonioUkuku_001
    :documentaA :FestividadQoylluriti ;
    :capturadoEnSegmento :MomentoAlba .

#################################################################
### 6. EJEMPLOS DE TEXTO PARA EMBEDDINGS
#################################################################

:PersonajeUkuku
    :descripcionBreve "Figura ritual que participa como guardián, penitente y mediador en la peregrinación al Señor de Qoyllur Rit’i."@es ;
    :descripcionEtnografica "El Ukuku combina devoción, disciplina y juego. Porta máscara de oso, vestimenta gruesa de lana y suele custodiar a los peregrinos en los tramos de mayor riesgo del camino hacia el santuario."@es ;
    :fuenteTexto "Notas de trabajo de campo, Qoyllur Rit’i, campaña 2023."@es .

:FestividadQoylluriti
    :descripcionBreve "Peregrinación andina al santuario del Señor de Qoyllur Rit’i, que reúne a naciones de danzantes y comunidades de la región cusqueña."@es ;
    :fuenteTexto "Resumen elaborado para el proyecto kg-llm-festividades."@es .
